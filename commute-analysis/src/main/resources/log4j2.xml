<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
  <Appenders>
    <File fileName="logs/commute-analyzer.log" name="applog">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root additivity="false" level="debug">
      <AppenderRef ref="applog"/>
    </Root>
  </Loggers>
</Configuration>